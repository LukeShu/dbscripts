pkgbase=pkg-split-a
pkgname=('pkg-split-a1' 'pkg-split-a2')
pkgver=1
pkgrel=1
pkgdesc="A split package called ${pkgbase}"
arch=('i686' 'x86_64')
url='http://www.archlinux.org/'
license=('GPL')
depends=('glibc')
options=(!strip)

package_pkg-split-a1() {
	install -D -m755 /bin/true "${pkgdir}"/usr/bin/${pkgname}
}

package_pkg-split-a2() {
	install -D -m755 /bin/true "${pkgdir}"/usr/bin/${pkgname}
}
